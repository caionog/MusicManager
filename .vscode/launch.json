    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - Applications",
            "request": "launch",
            "mainClass": "gui.Applications",
            "projectName": "MusicManager",
            "vmArgs": "--module-path ${PATH_TO_FX} --add-modules javafx.controls,javafx.fxml"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "--module-path ${PATH_TO_FX} --add-modules javafx.controls,javafx.fxml"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-MainScreen<MusicManager>",
            "request": "launch",
            "mainClass": "gui.MainScreen",
            "projectName": "MusicManager",
            "vmArgs": "--module-path ${PATH_TO_FX} --add-modules javafx.controls,javafx.fxml"
        }
    ]
}